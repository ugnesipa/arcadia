            },
            "post": {
                "tags": [
                    "Plants"
                ],
                "summary": "Create a new Plant",
                "description": "Stores the plant in the DB",
                "operationId": "storePlant",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "category",
                                    "origin",
                                    "climate",
                                    "mainenace_rating",
                                    "description"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Sample Name"
                                    },
                                    "category": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "House Plant"
                                    },
                                    "origin": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "East Asia"
                                    },
                                    "climate": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Temperate"
                                    },
                                    "maintenance_rating": {
                                        "type": "integer",
                                        "format": "integer",
                                        "example": "1"
                                    },
                                    "description": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "A description about this lovely plant"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "integer",
                                            "example": ""
                                        },
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
