        "/api/plants": {
            "get": {
                "tags": [
                    "Plants"
                ],
                "summary": "Display a listing of the resource.",
                "description": "Displays all plants",
                "operationId": "6e2a3a91967b1e364c87b755cd5152f6",
                "responses": {
                    "200": {
